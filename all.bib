  keywords	= {2013 zaveri nowick lehmann group_aksw
		  sys:relevantFor:infai sys:relevantFor:bis MOLE cogevo
		  sys:relevantFor:geoknow},
  owner		= {Amrapali},
  url		= {http://jens-lehmann.org/files/2013/odls_cogevo.pdf}
}

