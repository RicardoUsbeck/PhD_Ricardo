@InProceedings{	  frischmuth2103a,
  author	= {Philipp Frischmuth and Sören Auer and Sebastian Tramp and
		  Jörg Unbehauen and Kai Holzweißig and Car-Martin
		  Marquardt},
  title		= {Towards Linked Data based Enterprise Information
		  Integration},
  booktitle	= {Proceedings of the Workshop on Semantic Web Enterprise
		  Adoption and Best Practice (WASABI) 2013},
  year		= {2013},
  abstract	= {Data integration in large enterprises is a crucial but at
		  the same time costly, long lasting and challenging problem.
		  In the last decade, the prevalent data integration
		  approaches were primarily based on XML, Web Services and
		  Service Oriented Architectures (SOA). We argue that classic
		  SOA architectures may be well-suited for transaction
		  processing, however more efficient technologies can be
		  employed for enterprise data integration. In particular,
		  the use of the Linked Data paradigm appears to be a very
		  promising approach. In this article we explore challenges
		  large enterprises are still facing with regard to data
		  integration. We discuss Linked Data approaches in these
		  areas and present some examples of successful applications
		  of the Linked Data principles in that context.},
  keywords	= {2013 group_aksw auer peer-reviewed frischmuth},
  owner		= {philipp},
  timestamp	= {2013.10.26},
  url		= {http://www.wasabi-ws.org/papers/wasabi03/paper.pdf}
}

